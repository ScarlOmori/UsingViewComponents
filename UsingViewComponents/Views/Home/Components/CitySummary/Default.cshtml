@model CityViewModel

<table class="table table-sm table-bordered">
    <tr>
        <td>
            Citites:
        </td>
        <td class="text-right">
            @Model.Cities
        </td>
    </tr>
    <tr>
        <td>
            Population:
        </td>
        <td>
            @Model.Population.ToString("#,###");
        </td>
    </tr>
</table>